{
  "name": "casts",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "build:client": "webpack -p --mode production --env production --config webpack.client.js",
    "build:server": "webpack -p --mode production --config webpack.server.js",
    "build": "concurrently \"yarn:build:client\" \"yarn:build:server\"",
    "dev": "webpack-dev-server --open --mode development --config webpack.client.js",
    "start": "node build/server.js"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "reselect": "^4.0.0",
    "serialize-javascript": "^4.0.0",
    "serve-favicon": "^2.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "eslint": "^7.5.0",
    "eslint-plugin-react": "^7.20.3",
    "html-webpack-plugin": "^4.3.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-merge": "^5.0.9",
    "webpack-node-externals": "^2.5.0"
